@model Address
@{
    ViewData["Title"] = "Adress Summary";
}
<h1>@ViewData["Title"]</h1>

<table class="table table-striped">
    <tr>
        <td>First Name:</td>
        <td>@Model.FirstName</td>
    </tr>
    <tr>
        <td>Last Name:</td>
        <td>@Model.LastName</td>
    </tr>
    <tr>
        <td>Street:</td>
        <td>@Model.Street</td>
    </tr>
    <tr>
        <td>City:</td>
        <td>@Model.City</td>
    </tr>
    <tr>
        <td>Province:</td>
        <td>@Model.Province</td>
    </tr>
    <tr>
        <td>Postal Code:</td>
        <td>@Model.PostalCode</td>
    </tr>
    <tr>
        <td>Country:</td>
        <td>@Model.Country</td>
    </tr>
</table>
