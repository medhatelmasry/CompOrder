@model ContactSales
@{
    ViewData["Title"] = "Contact Summary";
}
<h1>@ViewData["Title"]</h1>

<table class="table table-striped table-sm">
    <tr>
        <td>First Name:</td>
        <td>@Model.FirstName</td>
    </tr>
    <tr>
        <td>Last Name:</td>
        <td>@Model.LastName</td>
    </tr>
    <tr>
        <td>Email:</td>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <td>Message:</td>
        <td>@Model.Message</td>
    </tr>

</table>
